  "scripts": {
    "//": [
      "nodemon => babel-node: runs the server with auto-rebuild (via babel) on change.",
      "start => runs the server with hot module replacement (server not updated).",
      "NOTE: nodemon seems to cache .graphql imports.",
      "NOTE: npm run server won't work without symlinks."
    ],
    "webpack": "webpack --watch",
    "webpack-server": "webpack --config webpack-server.config.js",
    "babel-node": "BABEL_DISABLE_CACHE=1 babel-node src/server/main.js",
    "nodemon": "nodemon --config nodemon.json --exec npm run babel-node",
    "server": "webpack --config webpack-server.config.js && node dist/server.bundle.js",
    "start": "BABEL_DISABLE_CACHE=1 BABEL_ENV=hot NODE_ENV=hot babel-node src/server/main.js"
