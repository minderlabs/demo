{
  "Group": [
    {
      "id": "minderlabs",
      "title": "Minder Labs"
    }
  ],

  "Project": [
    {
      "id": "demo",
      "title": "Demo",
      "team": "minderlabs",
      "boards": [
        {
          "alias": "tasks",
          "title": "Task Pipeline",
          "columns": [
            { "id": "c1", "value": { "int": 0 }, "title": "Icebox"    },
            { "id": "c2", "value": { "int": 1 }, "title": "Assigned"  },
            { "id": "c3", "value": { "int": 2 }, "title": "Active"    },
            { "id": "c4", "value": { "int": 3 }, "title": "Complete"  }
          ]
        }
      ]
    }
  ],

  "Folder": [
    {
      "id": "folder_inbox",
      "order": 1,
      "alias": "inbox",
      "title": "Inbox",
      "icon": "inbox",
      "filter": {
        "matchAll": true,
        "expr": {
          "op": "AND",
          "expr": [
            {
              "comp": "NE",
              "field": "type",
              "value": {
                "string": "Folder"
              }
            },
            {
              "comp": "GT",
              "field": "modified",
              "value": {
                "timestamp": -100000
              }
            }
          ]
        },
        "orderBy": {
          "field": "modified",
          "order": "DESC"
        },
        "groupBy": false
      }
    },
    {
      "id": "folder_tasks",
      "order": 2,
      "alias": "tasks",
      "title": "Tasks",
      "icon": "assignment_turned_in",
      "filter": {
        "type": "Task",
        "expr": {
          "op": "OR",
          "expr": [
            { "ref": "$USER_ID", "field": "owner" },
            { "ref": "$USER_ID", "field": "assignee" }
          ]
        },
        "groupBy": false
      }
    },
    {
      "id": "folder_favorites",
      "order": 3,
      "alias": "favorites",
      "title": "Favorites",
      "icon": "star",
      "filter": {
        "labels": ["_favorite"],
        "groupBy": false
      }
    },
    {
      "id": "folder_projects",
      "order": 4,
      "alias": "projects",
      "title": "Projects",
      "icon": "assignment",
      "filter": {
        "type": "Project",
        "groupBy": false
      }
    },
    {
      "id": "folder_deleted",
      "order": 5,
      "alias": "deleted",
      "title": "Deleted",
      "icon": "delete",
      "filter": {
        "labels": ["_deleted"],
        "groupBy": false
      }
    }
  ],

  "Contact": [
    {
      "id": "contact_alice",
      "title": "Alice Braintree",
      "email": "alice.braintree@gmail.com"
    }
  ]
}
